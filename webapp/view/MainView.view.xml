<mvc:View
    controllerName="logaligroup.projectlogali.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
                    <Label text="RUC"/>
					<Input value="{SupplierName}" required="true" enabled="true"/>
                    </content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
